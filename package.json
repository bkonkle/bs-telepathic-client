{
  "name": "bs-telepathic",
  "version": "2.8.0",
  "description": "A telepathic connection to your users, presented with a Node/JS and ReasonML interface",
  "main": "./dist/Telepathic.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/bs-telepathic.git"
  },
  "author": "Brandon Konkle <brandon@ecliptic.io> (http://ecliptic.io)",
  "license": "Apache-2.0",
  "keywords": [
    "bucklescript",
    "reasonml",
    "slack"
  ],
  "scripts": {
    "build": "yarn build.re && yarn build.bundle",
    "build.bundle": "webpack",
    "build.re": "bsb -make-world",
    "build.re.clean": "bsb -clean-world",
    "build.re.watch": "yarn build.re -w",
    "precommit": "lint-staged",
    "prepublish": "yarn build.re"
  },
  "dependencies": {
    "@glennsl/bs-json": "^1.1.2",
    "body-parser": "^1.18.2",
    "bs-abstract": "^0.12.0",
    "bs-node-debug": "^0.2.0",
    "bs-websockets": "git://github.com/bkonkle/bs-websockets.git#9a47c2d",
    "dotenv": "^5.0.1",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "bs-platform": "2.2.2",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "lint-staged": {
    "src/**/*.re": [
      "refmt --in-place",
      "git add"
    ]
  }
}
